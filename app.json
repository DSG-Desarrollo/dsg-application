{
  "expo": {
    "extra": {
      "eas": {
        "projectId": "f990977d-e454-427d-865b-27481130d47a"
      }
    },
    "name": "DSG",
    "slug": "dsg-application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dsg.application",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher/res/mipmap-hdpi/ic_launcher_adaptive_fore.png",
        "backgroundImage": "./assets/ic_launcher/res/mipmap-hdpi/ic_launcher_adaptive_back.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow dsg-application to access your camera",
          "microphonePermission": "Allow dsg-application to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}